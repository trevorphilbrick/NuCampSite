{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "This is a website for a fictional campsites review site, Nucamp",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run imagemin && npm run usemin && npm run copyfonts && npm run copyboot && npm run copysocial && npm run copyfa && npm run copyjquery && npm run copypopper && npm run copybootjs && npm run copyhtml",
    "clean": "rimraf dist",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    // "copycss": "copyfiles -f css/styles.css dist/css",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "copyfa": "copyfiles -f node_modules/font-awesome/css/font-awesome.min.css dist/css",
    "copyboot": "copyfiles -f node_modules/bootstrap/dist/css/bootstrap.min.css dist/css",
    "copysocial": "copyfiles -f node_modules/bootstrap-social/bootstrap-social.css dist/css",
    "copyjquery": "copyfiles -f node_modules/jquery/dist/jquery.slim.min.js dist/js",
    "copypopper": "copyfiles -f node_modules/popper.js/dist/umd/popper.min.js dist/js",
    "copybootjs": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.min.js dist/js",
    "copyhtml": "copyfiles index.html dist",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "Trevor Philbrick",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.0.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}